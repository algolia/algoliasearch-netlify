{
  "name": "@algolia/netlify-plugin-crawler",
  "version": "0.0.5",
  "private": false,
  "author": "Algolia Team <contact@algolia.com>",
  "license": "MIT",
  "repository": "https://github.com/algolia/algoliasearch-netlify.git",
  "bugs": {
    "url": "https://github.com/algolia/algoliasearch-netlify/issues"
  },
  "main": "dist/index.js",
  "files": [
    "README.md",
    "manifest.yml",
    "dist/"
  ],
  "scripts": {
    "build": "npx tsc -b",
    "dev": "ALGOLIA_DEV_ENV=development nodemon --config .nodemon.json --exec '../scripts/dev_plugin.sh'",
    "postinstall": "[ -d dist/ ] || npm run build",
    "release": "yarn build && yarn publish"
  },
  "dependencies": {
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/node-fetch": "2.5.7"
  },
  "keywords": [
    "netlify",
    "netlify-plugin",
    "search",
    "crawl",
    "crawler",
    "robots",
    "indexing",
    "netlify-search",
    "algolia",
    "algoliasearch"
  ]
}
